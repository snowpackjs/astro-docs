{
  "$schema": "./node_modules/@lunariajs/core/config.schema.json",
  "repository": {
    "name": "withastro/docs",
    "branch": "main",
    "hosting": "github"
  },
  "defaultLocale": {
    "label": "English",
    "lang": "en",
    "content": {
      "location": "src/(i18n/en/nav|content/docs/en/**/*).(ts|mdx)"
    },
    "dictionaries": {
      "location": "src/i18n/en/(docsearch|ui).ts",
      "optionalKeys": {
        "src/i18n/docsearch.ts": ["modal"]
      }
    }
  },
  "locales": [
